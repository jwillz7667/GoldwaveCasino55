{
    "name": "casino-platform",
    "version": "1.0.0",
    "description": "A comprehensive casino gaming platform with admin management",
    "main": "server/server.js",
    "scripts": {
        "start": "node server/server.js",
        "dev": "nodemon server/server.js",
        "test": "jest --coverage",
        "lint:backend": "eslint server --fix",
        "lint:frontend": "eslint src --fix",
        "lint": "npm run lint:backend && npm run lint:frontend",
        "format": "prettier --write .",
        "build": "webpack --mode production",
        "seed": "node scripts/seed.js"
    },
    "dependencies": {
        "bcrypt": "^5.1.1",
        "bcryptjs": "^2.4.3",
        "compression": "^1.7.5",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-fileupload": "^1.4.0",
        "express-rate-limit": "^6.7.0",
        "helmet": "^6.1.5",
        "hi-base32": "^0.5.1",
        "ioredis": "^5.4.2",
        "jsonwebtoken": "^9.0.0",
        "mongoose": "^7.0.3",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "on-finished": "^2.4.1",
        "on-headers": "^1.0.2",
        "otplib": "^12.0.1",
        "qrcode": "^1.5.3",
        "rate-limit-redis": "^4.2.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "redis": "^4.7.0",
        "sharp": "^0.32.0",
        "speakeasy": "^2.0.0",
        "sqlite3": "^5.1.6",
        "winston": "^3.17.0",
        "ws": "^8.13.0"
    },
    "devDependencies": {
        "@babel/core": "^7.21.4",
        "@babel/eslint-parser": "^7.25.9",
        "@babel/preset-env": "^7.21.4",
        "@types/jest": "^29.5.0",
        "@types/node": "^18.15.11",
        "babel-loader": "^9.1.2",
        "chart.js": "^4.4.7",
        "eslint": "^8.38.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-jest": "^27.2.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-react": "^7.37.3",
        "html-webpack-plugin": "^5.6.3",
        "husky": "^9.1.7",
        "jest": "^29.5.0",
        "lint-staged": "^15.2.11",
        "nodemon": "^3.1.9",
        "prettier": "^2.8.7",
        "supertest": "^6.3.3",
        "webpack": "^5.79.0",
        "webpack-cli": "^5.0.1"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "jest": {
        "testEnvironment": "node",
        "coverageDirectory": "./coverage",
        "collectCoverageFrom": [
            "server/**/*.js",
            "!server/server.js",
            "!server/config/*.js"
        ]
    },
    "nodemonConfig": {
        "watch": [
            "server"
        ],
        "ext": "js,json",
        "ignore": [
            "node_modules",
            "coverage",
            "public"
        ]
    },
    "prettier": {
        "singleQuote": true,
        "trailingComma": "es5",
        "tabWidth": 4,
        "semi": true,
        "printWidth": 100
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yourusername/casino-platform.git"
    },
    "keywords": [
        "casino",
        "gaming",
        "admin",
        "platform",
        "nodejs",
        "express",
        "mongodb"
    ],
    "author": "Your Name",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/yourusername/casino-platform/issues"
    },
    "homepage": "https://github.com/yourusername/casino-platform#readme",
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.js": [
            "eslint --fix",
            "prettier --write"
        ],
        "server/**/*.js": [
            "eslint --fix",
            "prettier --write"
        ]
    }
}
